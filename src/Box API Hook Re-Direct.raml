#%RAML 0.8
title: Box API Hook Re-Direct
version: 1.0
baseUri: http://localhost:9905/box_hook
/auth_success:
  displayName: Re-direct URL for box Auth server
  get:
    description: <<send auth token and status>>
    queryParameters:
      code:
        type: string
        required: true
      status:
        type: string
        required: true